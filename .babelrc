{
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }],
    "transform-class-properties",
    "syntax-dynamic-import"
  ],
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 1 versions"]
      },
      "exclude": ["transform-regenerator"],
      "modules": false,
      "useBuiltIns": true
    }],
    "react"
  ],

  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
